public class FormUpdateTurma {
	
	private Integer semestre;
	private Integer ano;	
	
	public FormUpdateTurma() {
	}	
	
	public Integer getSemestre() {
		return semestre;
	}
	public void setSemestre(Integer semestre) {
		this.semestre = semestre;
	}
	public Integer getAno() {
		return ano;
