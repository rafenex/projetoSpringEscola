package com.escola.escola.form;

import com.escola.escola.model.CCurso;
import com.escola.escola.model.CProfessor;

public class FormCadastroTurma {
	private Integer semestre;
	private Integer ano;	
	private CCurso curso;
	private CProfessor professor;
	
	public FormCadastroTurma() {
		// TODO Auto-generated constructor stub
	}

	public FormCadastroTurma(Integer semestre, Integer ano) {
		super();
		this.semestre = semestre;
		this.ano = ano;
	}

	public Integer getSemestre() {
		return semestre;
	}

	public void setSemestre(Integer semestre) {
		this.semestre = semestre;
	}

	public Integer getAno() {
		return ano;
	}

	public void setAno(Integer ano) {
		this.ano = ano;
	}
	
	
}
